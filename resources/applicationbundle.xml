<?xml version="1.0"?>
<bundle>
  <executables>
    <executable name="Inkscape">inkscape</executable>
    <executable>inkview</executable>
    <executable>fig2dev</executable>
    <executable>gs</executable>
    <executable>gtk4-demo</executable>
    <symlink>../Frameworks/Python.framework/Versions/Current/bin/python3</symlink>
  </executables>
  <frameworks>
    <framework>tmp/Python.framework</framework>
  </frameworks>
  <gir command="jhb run g-ir-compiler" lib_dir="@rpath"/>
  <gtk4/>
  <icons>
    <icon>tmp/Inkscape.icns</icon>
  </icons>
  <libraries>
    <library>libgtksourceview-5.0.dylib</library>
    <library>libreadline.8.dylib</library>
  </libraries>
  <locales>
    <locale>inkscape.mo</locale>
    <locale>gtksourceview-5.mo</locale>
  </locales>
  <plist/>
  <resources>
    <resource>etc/fonts</resource>
    <resource>share/fontconfig</resource>
    <resource>share/ghostscript</resource>
    <resource>share/gtksourceview-5</resource>
    <resource>share/ImageMagick-6</resource>
    <resource>share/icons</resource>
    <resource>share/inkscape</resource>
    <resource>share/themes</resource>
  </resources>
</bundle>
